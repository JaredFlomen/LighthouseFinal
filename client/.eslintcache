e\n  const { mode } = useVisualMode();\n\n  return <article classname=\"review\" data-testid=\"review\">\n    {mode === REVIEW1 && <ReviewShow />}\n    <ReviewShow />\n  </article>\n};","/vagrant/final_project/client/src/components/Review/ReviewForm.js",[],"/vagrant/final_project/client/src/components/StarRating.js",["46"],"import React, { useState } from 'react';\n\nimport { FaStar } from 'react-icons/fa'\n\nimport \"./StarRating.css\";\n\n\nexport default function StarRating(props) {\n  const [ rating, setRating ] = useState(null);\n  const [ hover, setHover ] = useState(null);\n\n  return (\n  <div>\n    {[ ... Array(5)].map((star, i) => {\n      const ratingValue = i + 1\n      return <label>\n        <input\n          type='radio'\n          name='rating'\n          value={ratingValue}\n          onClick={() => setRating(ratingValue)}\n        />\n        <FaStar\n          className='star'\n          color={ratingValue <= (hover || rating) ? \"gold\" : \"rgba(128, 128, 128, 0.246)\"}\n          size={50}\n          onMouseEnter={() => setHover(ratingValue)}\n          onMouseLeave={() => setHover(null)}\n        />\n      </label>;\n    })}\n  </div>\n  );\n};","/vagrant/final_project/client/src/components/Review/ReviewShow.js",["47","48"],"/vagrant/final_project/client/src/components/Map.js",[],"/vagrant/final_project/client/src/index.js",[],"/vagrant/final_project/client/src/reportWebVitals.js",[],"/vagrant/final_project/client/src/components/StarRatingShow.js",["49"],{"ruleId":"50","severity":1,"message":"51","line":4,"column":8,"nodeType":"52","messageId":"53","endLine":4,"endColumn":11},{"ruleId":"54","replacedBy":"55"},{"ruleId":"56","replacedBy":"57"},{"ruleId":"50","severity":1,"message":"58","line":3,"column":8,"nodeType":"52","messageId":"53","endLine":3,"endColumn":18},{"ruleId":"59","severity":1,"message":"60","line":14,"column":11,"nodeType":"61","messageId":"62","endLine":14,"endColumn":12,"fix":"63"},{"ruleId":"50","severity":1,"message":"64","l